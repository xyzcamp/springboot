<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="sample05.mybatis.MemberMapper">
	<select id="findOne" resultType="sample05.mybatis.MemberPO">
		select
			m_id, name
		from MemberTest
		where m_id = #{id}
	</select>

	<select id="findAll" resultType="sample05.mybatis.MemberPO">
		select
			*
		from MemberTest
		${_orderby}
	</select>

</mapper>
